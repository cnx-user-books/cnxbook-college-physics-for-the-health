                  <m:mtext>.</m:mtext>
                  <m:mrow>
                    <m:mn>1</m:mn>
                    <m:mo stretchy="false">×</m:mo>
                    <m:msup>
                      <m:mtext>10</m:mtext>
                      
                        <m:mrow>
